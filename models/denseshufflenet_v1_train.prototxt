  top: "conv2_1/x3"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }

  convolution_param {
    num_output: 8      # Down Channel to 8

    bias_term: false
    kernel_size: 1
    stride: 1
    pad: 0

    dilation: 1

    # group: 4             # Group Pointwise Convolution

    weight_filler {
      type: "msra"
    }
  }
}
layer {
  name: "conv2_1/x3/bn"
  type: "BatchNorm"

  bottom: "conv2_1/x3"
  top: "conv2_1/x3"

  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }

  batch_norm_param {
    use_global_stats: false
  }
}
layer {
  name: "conv2_1/x3/scale"
  type: "Scale"

  bottom: "conv2_1/x3"
  top: "conv2_1/x3"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }
  param {
    lr_mult: 2.0
    decay_mult: 0.0
  }

  scale_param {
    bias_term: true
  }
}

layer {
  name: "concat_2_1"
  type: "Concat"

  bottom: "pooling2/add"
  bottom: "conv2_1/x3"
  top: "concat_2_1"
}
layer {
  name: "relu2_1/x2"
  type: "ReLU"

  bottom: "concat_2_1"
  top: "concat_2_1"

  relu_param {
    negative_slope: 0.10
  }
}


}
layer {
  name: "conv2_2/x1/bn"
  type: "BatchNorm"

  bottom: "conv2_2/x1"
  top: "conv2_2/x1"

  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {

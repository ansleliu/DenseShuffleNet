name: "DENSE-SHUFFLE-NET"

layer {
  name: "data"
  type: "Data"

  top: "data"

  include {
    phase: TRAIN
  }

  data_param {
    source: "/media/datavolume3/huijun/CityScapes/cityscapes_lmdb_val/data"
    backend: LMDB
    batch_size: 1
  }

  transform_param {
    scale: 0.00390625         # feature scaling coefficient: this maps the [0, 255] data to [0, 1]
    mean_value: 80.5423       # B
    mean_value: 91.3162       # G
    mean_value: 81.4312       # R
  }
}

layer {
  name: "label"
  type: "Data"

  top: "label"

  include {
    phase: TRAIN
  }

  data_param {
    source: "/media/datavolume3/huijun/CityScapes/cityscapes_lmdb_val/label"
    backend: LMDB
    batch_size: 1
  }
}

# ------------------------------ Net Define ------------------------------ #
# ------------------------------ Pooling 0  1/2
layer {
  name: "conv1"
  type: "Convolution"      # Pooling 1/2

  bottom: "data"
  top: "conv1"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }

  convolution_param {
    num_output: 64

    pad: 3
    stride: 2
    kernel_size: 7

    dilation: 1

    weight_filler {
      type: "msra"
    }
    bias_term: false
  }
}
layer {
  name: "conv1/bn"
  type: "BatchNorm"

  bottom: "conv1"
  top: "conv1"

  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }

  batch_norm_param {
    use_global_stats: false
  }
}
layer {
  name: "conv1/scale"
  type: "Scale"

  bottom: "conv1"
  top: "conv1"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }
  param {
    lr_mult: 2.0
    decay_mult: 0.0
  }

  scale_param {
    bias_term: true
  }
}
layer {
  name: "relu1"
  type: "ReLU"

  bottom: "conv1"
  top: "conv1"

  relu_param {
    negative_slope: 0.10
  }
}

# --------------------------- Pooling 1  1/4
layer {
    name: "pool2"
    type: "Pooling"

    bottom: "conv1"
    top: "pool2"
    pooling_param {
        kernel_size: 3
        stride: 2
        pool: MAX
    }
}

layer {
  name: "conv2/x0"
  type: "Convolution"

  bottom: "conv1"
  top: "conv2/x0"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }

  convolution_param {
    num_output: 128

    pad: 0
    stride: 1
    kernel_size: 1

    group: 4

    dilation: 1

    weight_filler {
      type: "msra"
    }
    bias_term: false
  }
}
layer {
  name: "conv2/x0/bn"
  type: "BatchNorm"

  bottom: "conv2/x0"
  top: "conv2/x0"

  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }

  batch_norm_param {
    use_global_stats: false
  }
}
layer {
  name: "conv2/x0/scale"
  type: "Scale"

  bottom: "conv2/x0"
  top: "conv2/x0"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }
  param {
    lr_mult: 2.0
    decay_mult: 0.0
  }

  scale_param {
    bias_term: true
  }
}
layer {
  name: "relu2/x0"
  type: "ReLU"

  bottom: "conv2/x0"
  top: "conv2/x0"

  relu_param {
    negative_slope: 0.10
  }
}
layer {
  name: "shuffle2/x0"
  type: "ShuffleChannel"

  bottom: "conv2/x0"
  top: "shuffle2/x0"

  shuffle_channel_param {
    group: 4
  }
}

layer {
  name: "conv2/x2"
  type: "ConvolutionDepthwise"

  bottom: "shuffle2/x0"
  top: "conv2/x2"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }

  convolution_param {
    num_output: 128

    pad: 1
    stride: 2
    kernel_size: 3

    dilation: 1

    weight_filler {
      type: "msra"
    }
    bias_term: false
  }
}
layer {
  name: "conv2/x2/bn"
  type: "BatchNorm"

  bottom: "conv2/x2"
  top: "conv2/x2"

  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }

  batch_norm_param {
    use_global_stats: false
  }
}
layer {
  name: "conv2/x2/scale"
  type: "Scale"

  bottom: "conv2/x2"
  top: "conv2/x2"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }
  param {
    lr_mult: 2.0
    decay_mult: 0.0
  }

  scale_param {
    bias_term: true
  }
}

layer {
  name: "conv2/x1"
  type: "Convolution"

  bottom: "conv2/x2"
  top: "conv2/x1"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }

  convolution_param {
    num_output: 64

    pad: 0
    stride: 1
    kernel_size: 1

    group: 4

    dilation: 1

    weight_filler {
      type: "msra"
    }
    bias_term: false
  }
}
layer {
  name: "conv2/x1/bn"
  type: "BatchNorm"

  bottom: "conv2/x1"
  top: "conv2/x1"

  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }

  batch_norm_param {
    use_global_stats: false
  }
}
layer {
  name: "conv2/x1/scale"
  type: "Scale"

  bottom: "conv2/x1"
  top: "conv2/x1"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }
  param {
    lr_mult: 2.0
    decay_mult: 0.0
  }

  scale_param {
    bias_term: true
  }
}

layer {
    name: "pooling2/add"
    type: "Eltwise"

    bottom: "pool2"
    bottom: "conv2/x1"
    top: "pooling2/add"

    eltwise_param {
        operation: SUM
    }
}

layer {
    name: "pool2/relu"
    type: "ReLU"

    bottom: "pooling2/add"
    top: "pooling2/add"

    relu_param {
        negative_slope: 0.10
    }
}

# ------------------------------ Dense Block #1 ----------------------------- #
# ------------------------------ DenseShuffle Unit #1 64+8
layer {
  name: "conv2_1/x1"
  type: "Convolution"

  bottom: "pooling2/add"
  top: "conv2_1/x1"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }

  convolution_param {
    num_output: 32      # Down Channel to 8

    bias_term: false
    kernel_size: 1
    stride: 1
    pad: 0

    dilation: 1

    group: 4             # Group Pointwise Convolution

    weight_filler {
      type: "msra"
    }
  }
}
layer {
  name: "conv2_1/x1/bn"
  type: "BatchNorm"

  bottom: "conv2_1/x1"
  top: "conv2_1/x1"

  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }

  batch_norm_param {
    use_global_stats: false
  }
}
layer {
  name: "conv2_1/x1/scale"
  type: "Scale"

  bottom: "conv2_1/x1"
  top: "conv2_1/x1"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }
  param {
    lr_mult: 2.0
    decay_mult: 0.0
  }

  scale_param {
    bias_term: true
  }
}
layer {
  name: "relu2_1/x1"
  type: "ReLU"

  bottom: "conv2_1/x1"
  top: "conv2_1/x1"

  relu_param {
    negative_slope: 0.10
  }
}
layer {
  name: "shuffle2_1/x1"
  type: "ShuffleChannel"      # Channel Shuffle

  bottom: "conv2_1/x1"
  top: "shuffle2_1/x1"

  shuffle_channel_param {
    group: 4
  }
}


layer {
  name: "conv2_1/x2"
  type: "ConvolutionDepthwise"  # Depthwise Convolution

  bottom: "shuffle2_1/x1"
  top: "conv2_1/x2"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }

  convolution_param {
    num_output: 32

    bias_term: false
    pad: 1
    stride: 1
    kernel_size: 3

    dilation: 1

    weight_filler {
      type: "msra"
    }
  }
}
layer {
  name: "conv2_1/x2/bn"
  type: "BatchNorm"

  bottom: "conv2_1/x2"
  top: "conv2_1/x2"

  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }

  batch_norm_param {
    use_global_stats: false
  }
}
layer {
  name: "conv2_1/x2/scale"

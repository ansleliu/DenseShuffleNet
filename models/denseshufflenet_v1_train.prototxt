  name: "conv2_2/x1"
  type: "Convolution"    # Group Pointwise Convolution

  bottom: "concat_2_1"
  top: "conv2_2/x1"

  param {
    lr_mult: 1.0
    decay_mult: 1.0
  }

  convolution_param {
    num_output: 32

    bias_term: false
    kernel_size: 1
    stride: 1
    pad: 0

    group: 4

    dilation: 1

    weight_filler {
      type: "msra"
